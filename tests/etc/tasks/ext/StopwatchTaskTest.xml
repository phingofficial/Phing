<project name="stopwatch test" default="testStopwatch">

    <target name="testStopwatch">
        <stopwatch name="test" category="test-cat" action="start"/>
        <sleep milliseconds="10"/>
        <stopwatch name="test" category="test-cat" action="lap"/>
